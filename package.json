{
  "name": "@precognitive/decision-ai-sdk",
  "version": "1.0.0",
  "description": "Node SDK for integrating the Precognitive Decision-AI APIs with Node based applications and tools.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Precognitive/nodejs-decision-ai.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "precognitive",
    "node",
    "auth0"
  ],
  "author": "@precognitive",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^12.7.1",
    "@types/node-fetch": "^2.3.2",
    "fetch-mock": "^7.3.1",
    "jest": "^24.7.1",
    "jest-junit": "^7.0.0",
    "jest-mock-console": "^0.4.2",
    "lodash": "^4.17.15",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/typescript": "^2.0.0",
    "node-fetch": "^2.3.0"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/**",
      "!**/__tests__/**",
      "!**/bin/*"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)"
    ],
    "preset": "ts-jest"
  },
  "files": [
    "dist"
  ]
}
